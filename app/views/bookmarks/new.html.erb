<div class="container">
  <div class="column">
  <h2> New Bookmark <h2>
  </div>
  <div class="column">
    <div class="row">
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.association :movie %>
        <%= f.input :comment %>
        <div class="form-actions">
        <%= f.button :submit, class:"btn btn-primary" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
